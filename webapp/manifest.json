{
  "_version": "1.17.0",
  "sap.app": {
    "id": "com.bmc.hcm.drf.zhcmuxdrf",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{APPLICATION_TITLE}}",
    "description": "{{APPLICATION_DESCRIPTION}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.3",
      "toolsId": "0fe72767-1f31-48b1-9856-e3ee93f12634"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZHCM_UX_DRF_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "ZHCM_GENERIC_SRV": {
        "uri": "/sap/opu/odata/sap/ZHCM_GENERIC_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/ZHCM_GENERIC_SRV/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.71.75",
      "libs": {
        "sap.collaboration": {
          "lazy": true
        },
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.bmc.hcm.drf.zhcmuxdrf.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultCountMode": "None"
        }
      },
      "GenericServices": {
        "uri": "/sap/opu/odata/sap/ZHCM_GENERIC_SRV/",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "OneWay",
          "defaultCountMode": "None"
        },
        "dataSource": "ZHCM_GENERIC_SRV",
        "preload": true
      }
    },
    "services": {
      "ShellUIService": {
        "factoryName": "sap.ushell.ui5service.ShellUIService",
        "lazy": false,
        "settings": {
          "setTitle": "auto"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/customDRF.css"
        }
      ],
      "js": [
        {
          "uri": "utils/lodash.js"
        },
        {
          "uri": "utils/material.js"
        },
        {
          "uri": "utils/FileDownloadJquery.js"
        },
        {
          "uri": "utils/moment.js"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.bmc.hcm.drf.zhcmuxdrf.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },

      "routes": [
        {
          "pattern": "",
          "name": "appdispatcher",
          "target": ["appdispatcher"]
        },
        {
          "pattern": "mngrequestlist",
          "name": "mngrequestlist",
          "target": ["requestlist"]
        },
        {
          "pattern": "recrequestlist",
          "name": "recrequestlist",
          "target": ["approvedrequestlist"]
        },
        {
          "pattern": "approvallist",
          "name": "approvallist",
          "target": ["approvallist"]
        },
        {
          "pattern": "employeerequestnew",
          "name": "employeerequestnew",
          "target": ["employeerequestnew"]
        },
        {
          "pattern": "employeerequestedit/{Drfid}",
          "name": "employeerequestedit",
          "target": [
              "employeerequestedit"
          ]
        }
      ],
      "targets": {
        "appdispatcher": {
          "viewName": "AppDispatcher",
          "viewId": "appdispatcher",
          "viewLevel": 1,
          "title": "{i18n>RECRUITMENT_APPLICATIONS}"
        },
        "approvedrequestlist": {
          "viewName": "ApprovedRequestList",
          "viewId": "approvedrequestlist",
          "viewLevel": 2,
          "title": "{i18n>EMPLOYEE_REQUEST_FORM_LIST}"
        },
        "requestlist": {
          "viewName": "RequestList",
          "viewId": "requestlist",
          "viewLevel": 2,
          "title": "{i18n>EMPLOYEE_REQUEST_FORM_LIST}"
        },
        "approvallist": {
          "viewName": "ApprovalList",
          "viewId": "approvallist",
          "viewLevel": 2,
          "title": "{i18n>REQUESTS_WAITING_APPROVAL}"
        },
        "employeerequestnew": {
          "viewName": "EmployeeRequestNew",
          "viewId": "employeerequestnew",
          "viewLevel": 3,
          "title": "{i18n>EMPLOYEE_REQUEST_FORM_DETAIL}"
        },
        "employeerequestedit": {
          "viewName": "EmployeeRequestEdit",
          "viewId": "employeerequestedit",
          "viewLevel": 3,
          "title": "{i18n>EMPLOYEE_REQUEST_FORM_DETAIL}"
        }
      }
    },
    "config": {
      "fullWidth": true
    },
    "rootView": {
      "viewName": "com.bmc.hcm.drf.zhcmuxdrf.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
